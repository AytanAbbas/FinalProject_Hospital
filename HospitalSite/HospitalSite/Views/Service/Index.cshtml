
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@model VmService
<main>
    <partial name="_BannerPartial" model="Model.Banner" />

    <div class="site-main">
        <section class="ttm-row introduction-section clearfix">
            <div class="container">
                <div class="row">
                    <div class="col-lg-6 col-xs-12">
                        <div class="pt-15 res-991-pb-40 res-991-pt-0">
                            <div class="section-title">
                                <div class="title-header">
                                    <h5>About Us</h5>
                                    <h2 class="title">@Model.ServiceAbout.Title</h2>
                                </div>
                                <div class="title-desc">@Model.ServiceAbout.About</div>
                            </div>

                            <div class="row">
                                <div class="col-lg-5 col-md-4 col-sm-6">
                                    <ul class="ttm-list ttm-list-style-icon ttm-textcolor-darkgrey">
                                        @foreach (var item in Model.ServiceTips.Take(6))
                                        {
                                            <li>
                                                <i class="fas fa-minus"></i>
                                                <div class="ttm-list-li-content">@item.Name</div>
                                            </li>
                                        }

                                    </ul>
                                </div>
                                <div class="col-lg-7 col-md-8 col-sm-6">
                                    <ul class="ttm-list ttm-list-style-icon ttm-textcolor-darkgrey res-991-pt-10">
                                        <li>
                                            <i class="fas fa-minus"></i>
                                            <div class="ttm-list-li-content">Sample Preparation</div>
                                        </li>
                                        <li>
                                            <i class="fas fa-minus"></i>
                                            <div class="ttm-list-li-content">Environmental Testing</div>
                                        </li>
                                        <li>
                                            <i class="fas fa-minus"></i>
                                            <div class="ttm-list-li-content">Anatomical Pathology</div>
                                        </li>
                                        <li>
                                            <i class="fas fa-minus"></i>
                                            <div class="ttm-list-li-content">Advanced Microscopy</div>
                                        </li>
                                        <li>
                                            <i class="fas fa-minus"></i>
                                            <div class="ttm-list-li-content">Chemical Research</div>
                                        </li>
                                    </ul>
                                </div>
                            </div>
                           
                        </div>
                    </div>
                    <div class="col-lg-6 col-sm-7 m-auto">
                        <div class="ttm_single_image-wrapper">
                            <img class="img-fluid" src="~/Uploads/@Model.ServiceAbout.Image" alt="">
                        </div>
                    </div>
                </div>
            </div>
        </section>

        <section class="ttm-row features-section ttm-bgcolor-skincolor bg-img4 ttm-bg ttm-bgimage-yes z-index-1 clearfix">
            <div class="ttm-row-wrapper-bg-layer ttm-bg-layer"></div>
            <div class="container">
                <div class="row">
                    @foreach (var item in Model.Futures)
                    {<div class="col-lg-4">
                            <div class="featured-icon-box icon-align-top-content text-center style2">
                                <div class="featured-icon">
                                    <div class="ttm-icon ttm-icon_element-border ttm-icon_element-color-white ttm-icon_element-size-md ttm-icon_element-style-round">
                                        <i class="@item.Icon"></i>
                                    </div>
                                </div>
                                <div class="featured-content">
                                    <div class="featured-title">
                                        <h5>@item.Title</h5>
                                    </div>
                                    <div class="featured-desc">
                                        <p>@item.Description</p>
                                    </div>
                                </div>

                            </div>
                        </div>

                    }

                </div>

            </div>
        </section>

        <section class="ttm-row services-section-2 explore bg-layer-equal-height ttm-bgcolor-grey bg-img5 ttm-bg ttm-bgimage-yes clearfix">
            <div class="ttm-row-wrapper-bg-layer ttm-bg-layer"></div>
            <div class="container">
                <div class="row">
                    <div class="col-lg-8 col-md-8 col-sm-8 m-auto">
                        <div class="section-title with-sep title-style-center_text">
                            <div class="title-header">
                                <h5>OUR SERVICES</h5>
                                <h2 class="title">Explore Our Main Services</h2>
                            </div>
                            <div class="title-desc">Our excellent team is ready to be part of your team to work into the different areas of diagnoses. Scientists helping scientists.</div>
                        </div>
                    </div>
                </div>
                <div class="row no-gutters">
                    <div class="col-xl-6 col-lg-6">
                        <div class="ttm-bgcolor-white pl-50 pt-50 pb-50 pr-50 res-991-pl-15 res-991-pr-15 res-991-pt-30 res-991-pb-40">
                            <h4>We Are The Trusted Experts Latest Laboratory Technologies.</h4>
                            <p class="pb-10">Our scientists and engineers focus their extreme curiosity on national and global security & health related challenges. Learn more about the Laboratory’s R&D work below segment.</p>
                            <div class="ttm-progress-bar" data-percent="88%">
                                <div class="progressbar-title">Sample Preparation</div>
                                <div class="progress-bar-inner">
                                    <div class="progress-bar progress-bar-color-bar_skincolor"></div>
                                </div>
                                <div class="progress-bar-percent" data-percentage="88">88%</div>
                            </div>
                            <div class="ttm-progress-bar clearfix" data-percent="96%">
                                <div class="progressbar-title">Environmental Testing</div>
                                <div class="progress-bar-inner">
                                    <div class="progress-bar progress-bar-color-bar_skincolor"></div>
                                </div>
                                <div class="progress-bar-percent" data-percentage="96">96%</div>
                            </div>
                            <div class="ttm-progress-bar clearfix" data-percent="90%">
                                <div class="progressbar-title">Advanced Microscopy</div>
                                <div class="progress-bar-inner">
                                    <div class="progress-bar progress-bar-color-bar_skincolor"></div>
                                </div>
                                <div class="progress-bar-percent" data-percentage="90">90%</div>
                            </div>
                            <div class="ttm-progress-bar clearfix" data-percent="90%">
                                <div class="progressbar-title">Advanced Microscopy</div>
                                <div class="progress-bar-inner">
                                    <div class="progress-bar progress-bar-color-bar_skincolor"></div>
                                </div>
                                <div class="progress-bar-percent" data-percentage="90">90%</div>
                            </div>
                        </div>
                        <div class="ttm-bgcolor-skincolor pt-15 pl-50 pb-5 mb-50 res-991-pl-15">
                            <h4 class="mb-5"> High Technology Laboratory solutions</h4>
                        </div>
                    </div>
                    <div class="col-xl-6 col-lg-6">
                        <div class="ttm-bgcolor-white">
                            <div class="row">
                                <div class="col-lg-8 col-md-6 col-sm-7 m-auto">
                                    <div class="ttm_single_image-wrapper mt-50 mb_50 mr_150 res-991-mr-15 z-index-2 position-relative">
                                        <img class="img-fluid" src="images/service/single-img-eight.jpg" alt="">
                                    </div>
                                </div>
                                <div class="col-lg-4">
                                    <div class="ttm-bg ttm-col-bgcolor-yes ttm-bgcolor-skincolor pt-50 pr-50 mb-50 ml_150">
                                        <div class="ttm-col-wrapper-bg-layer ttm-bg-layer"></div>
                                        <div class="layer-content">
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </section>

        <section class="ttm-row portfolio-section-2 clearfix">
            <div class="container">
                <div class="row">
                    <div class="col-lg-6 col-md-8 col-sm-8 m-auto">
                        <div class="section-title with-sep title-style-center_text">
                            <div class="title-header">
                                <h5>WHAT WE DO</h5>
                                <h2 class="title">Our Latest Case Studies</h2>
                            </div>
                            <div class="title-desc">Our collaboration projects between the public sector, industry and academia bring a real impact for the our health and lab firms.</div>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-lg-12">
                        <div class="ttm-tabs tabs-style-01">
                            <ul class="tabs portfolio-filter">
                                <li class="tab active"><a href="#" data-filter="*">All</a></li>
                                @foreach (var item in Model.Categories)
                                {
                                    <li class="tab"><a href="#" data-filter=".@item.Name">@item.Name</a></li>
                                }

                            </ul>
                            <div class="content-tab">
                                <div class="row multi-columns-row ttm-boxes-spacing-15px isotope-project">
                                    <div class="ttm-box-col-wrapper col-lg-4 col-md-4 col-sm-6 cardiology anestheslology">
                                        <div class="featured-imagebox featured-imagebox-portfolio ttm-portfolio-box-view1">
                                            <div class="ttm-box-view-overlay ttm-portfolio-box-view-overlay">
                                                <div class="featured-thumbnail">
                                                    <a href="#"> <img class="img-fluid" src="~/images/01.jpg" alt="image"></a>
                                                </div>
                                                <div class="featured-iconbox ttm-media-link">

                                                    <a asp-controller="Research" asp-action="Detail" class="ttm_link"><i style=" line-height: 2;" class="fal fa-link"></i></a>
                                                </div>
                                                <div class="ttm-box-view-content-inner">
                                                    <div class="featured-content featured-content-portfolio">
                                                        <div class="featured-title">
                                                            <h5><a asp-controller="Research" asp-action="Detail">Rehabilitation Center</a></h5>
                                                        </div>
                                                        <span class="category">Cardiology, Anestheslology</span>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="ttm-box-col-wrapper col-lg-4 col-md-4 col-sm-6 pediatric orthopedics">
                                        <div class="featured-imagebox featured-imagebox-portfolio ttm-portfolio-box-view1">
                                            <div class="ttm-box-view-overlay ttm-portfolio-box-view-overlay">
                                                <div class="featured-thumbnail">
                                                    <a href="#"> <img class="img-fluid" src="images/02.jpg" alt="image"></a>
                                                </div>
                                                <div class="featured-iconbox ttm-media-link">

                                                    <a asp-controller="Research" asp-action="Detail" class="ttm_link"><i style=" line-height: 2;" class="fal fa-link"></i></a>
                                                </div>
                                                <div class="ttm-box-view-content-inner">
                                                    <div class="featured-content featured-content-portfolio">
