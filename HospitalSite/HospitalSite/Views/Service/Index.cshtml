                    </div>
                </div>
            </div>
        </section>

        <div class="ttm-row client-section-3 clearfix">
            <div class="container">
                <div class="row text-center">
                    <div class="col-md-12">
                        <div class="slick_slider" data-slick='{"slidesToShow": 6, "slidesToScroll": 1, "arrows":false, "autoplay":true, "infinite":true}'>
                            @foreach (var item in Model.Partners)
                            {
                                <div class="client-box">
                                    <div class="ttm-client-logo-tooltip" data-tooltip="@item.HoverName">
                                        <div class="ttm-client-logo-tooltip-inner">
                                            <div class="client-thumbnail">
                                                <img src="images/service/@item.Name" alt="image">
                                            </div>
                                            <div class="client-thumbnail_hover">
                                                <img src="images/service/@item.Name" alt="image">
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            }

                        </div>
                    </div>
                </div>
            </div>
        </div>


    </div>
</main>


