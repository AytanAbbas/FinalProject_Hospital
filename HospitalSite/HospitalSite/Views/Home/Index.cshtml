                                        <div class="progress-bar-inner">
                                            <div class="progress-bar progress-bar-color-bar_skincolor" style="width: 92%;"></div>
                                        </div>
                                        <div class="progress-bar-percent" data-percentage="92">92%</div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>

