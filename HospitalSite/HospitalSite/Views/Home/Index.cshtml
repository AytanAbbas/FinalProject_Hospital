                    <div class="ttm-fid inside ttm-fid-with-icon ttm-fid-view-lefticon style1">
                        <div class="ttm-fid-icon-wrapper">
                            <i class="fal fa-search"></i>
                        </div>
                        <div class="ttm-fid-contents">
                            <h4 class="ttm-fid-inner">
                                <span data-appear-animation="animateDigits" data-from="0" data-to="675" data-interval="15" data-before="" data-before-style="sup" data-after="" data-after-style="sub" class="numinate">0</span>
                                <sub>+</sub>
                            </h4>
                            <h3 class="ttm-fid-title">Complete Cases</h3>
                        </div>
                    </div>

                </div>
