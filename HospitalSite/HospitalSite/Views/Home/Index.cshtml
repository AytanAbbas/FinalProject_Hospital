@model VmHome
<div class="site-main">


    <section class="ttm-row introduction-section clearfix">
        <div class="container">
            <div class="row no-gutters">
                <div class="col-xl-6 col-lg-5 col-xs-12">

                    <div class="ttm_single_image-wrapper mb-30">
                        <img style="padding-right: 60px;" class="img-fluid" src="images/about/@Model.Setting.AboutImage" alt="">
                    </div>
                </div>
                <div class="col-xl-6 col-lg-7 col-xs-12">
                    <div class="pt-5 res-991-pt-0">

                        <div class="section-title">
                            <div class="title-header">
                                <h5>About Us</h5>
                                <h2 class="title">We Employ The Latest Research Technology &amp; Company</h2>
                            </div>
                            <div class="title-desc">@Model.Setting.About</div>
                        </div>

                        <div class="featuredbox-number">
                            @foreach (var item in Model.AboutResearches)
                            {
                                <div class="featured-icon-box icon-align-before-content style2">
                                    <div class="featured-icon">
                                        <div class="ttm-icon ttm-icon_element-fill ttm-icon_element-color-grey ttm-icon_element-size-sm ttm-icon_element-style-rounded">
                                            @item.Number
                                        </div>
                                    </div>
                                    <div class="featured-content">
                                        <div class="featured-title">
                                            <h5>@item.Title</h5>
                                        </div>
                                        <div class="featured-desc">
                                            <p>@item.Content</p>
                                        </div>
                                    </div>
                                </div>
                            }

                        </div>
                    </div>
                </div>
            </div>
            <div class="row ttm-boxes-spacing-5px mt-20">
                <div class="col-lg-3 col-md-6 col-sm-6 ttm-box-col-wrapper">
                    <div class="ttm-fid inside ttm-fid-with-icon ttm-fid-view-lefticon style1">
                        <div class="ttm-fid-icon-wrapper">
                            <i class="fal fa-trophy"></i>
                        </div>
                        <div class="ttm-fid-contents">
                            <h4 class="ttm-fid-inner">
                                <span data-appear-animation="animateDigits" data-from="0" data-to="143" data-interval="15" data-before="" data-before-style="sup" data-after="" data-after-style="sub" class="numinate">0</span>
                                <sub>+</sub>
                            </h4>
                            <h3 class="ttm-fid-title">Awards Winning</h3>
                        </div>
                    </div>
                </div>
                <div class="col-lg-3 col-md-6 col-sm-6 ttm-box-col-wrapper">
                    <div class="ttm-fid inside ttm-fid-with-icon ttm-fid-view-lefticon style1">
                        <div class="ttm-fid-icon-wrapper">
                            <i class="fal fa-microscope"></i>
                        </div>
                        <div class="ttm-fid-contents">
                            <h4 class="ttm-fid-inner">
                                <span data-appear-animation="animateDigits" data-from="0" data-to="780" data-interval="15" data-before="" data-before-style="sup" data-after="" data-after-style="sub" class="numinate">0</span>
                                <sub>+</sub>
                            </h4>
                            <h3 class="ttm-fid-title">Our Equipments</h3>
                        </div>
                    </div>
                </div>
                <div class="col-lg-3 col-md-6 col-sm-6 ttm-box-col-wrapper">
                    <div class="ttm-fid inside ttm-fid-with-icon ttm-fid-view-lefticon style1">
                        <div class="ttm-fid-icon-wrapper">
                            <i class="fal fa-search"></i>
                        </div>
                        <div class="ttm-fid-contents">
                            <h4 class="ttm-fid-inner">
                                <span data-appear-animation="animateDigits" data-from="0" data-to="675" data-interval="15" data-before="" data-before-style="sup" data-after="" data-after-style="sub" class="numinate">0</span>
                                <sub>+</sub>
                            </h4>
                            <h3 class="ttm-fid-title">Complete Cases</h3>
                        </div>
                    </div>

                </div>
                <div class="col-lg-3 col-md-6 col-sm-6 ttm-box-col-wrapper">
