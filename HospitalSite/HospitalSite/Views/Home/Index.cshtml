                                            <i class="ttm-num two ti-info"></i>
                                        </div>
                                    </div>
                                    <div class="featured-content">
                                        <div class="featured-title">
                                            <h5>@item.Title</h5>
                                        </div>
                                        <div class="featured-desc">
                                            <p>@item.Content</p>
                                        </div>
                                    </div>
                                </div>
                            }

                        </div>
                    </div>
                </div>
            </div>
